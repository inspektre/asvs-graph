{
  "name": "apimigration",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "./node_modules/.bin/nodemon --watch src --ext ts,graphql --exec ts-node-dev --transpile-only --no-notify --exit-child src/index.ts",
    "prettier-format": "prettier 'src/**/*.ts' --write",
    "prepare": "ts-node src/prepare.ts",
    "start": "node dist/src/index.js",
    "build": "yarn clean && tsc && yarn compile",
    "compile": "cd dist && pkg index.js -t node16-linux-x64 -o asvs-graphql --compress GZip && cd .. && mv dist/asvs-graphql artefacts/asvs-graphql",
    "build-macos": "yarn clean && tsc && cd dist && pkg index.js -o inspektre-api-macos -t node16-macos-arm64 && cd ..",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "deploy": "yarn build && zip -rq artefacts.zip artefacts && ./scripts/deploy.sh && rm -rf artefacts.zip"
  },
  "dependencies": {
    "@neo4j/graphql": "^3.2.2",
    "@neo4j/graphql-ogm": "^3.2.2",
    "@neo4j/graphql-plugin-auth": "^1.0.0",
    "@types/ws": "^8.5.3",
    "apollo-server": "^3.7.0",
    "apollo-server-express": "^3.7.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.8.2",
    "jsonwebtoken": "^8.5.1",
    "neo4j-driver": "^4.4.5",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-graphql": "^0.9.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.34",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.6.2",
    "shx": "^0.3.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
